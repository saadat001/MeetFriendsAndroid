<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="225"
                android:centerColor="@color/colorMedium"
                android:centerX="45%"
                android:endColor="@color/colorOrange"
                android:startColor="@color/colorPurple"
                android:type="linear" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/dimen_1"
        android:left="@dimen/dimen_1"
        android:right="@dimen/dimen_1"
        android:top="@dimen/dimen_1">
        <shape android:shape="oval">

            <solid android:color="@android:color/darker_gray"/>
            <stroke
                android:width="1dp"
                android:color="#FFFFFF" />
        </shape>
    </item>
</layer-list>


